CREATE TABLE Weather (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    searchText TEXT UNIQUE,
    name TEXT NOT NULL,
    temp REAL NOT NULL,
    speed REAL NOT NULL,
    humidity INTEGER NOT NULL,
    pressure INTEGER NOT NULL
);

getAllWeather:
SELECT *
FROM Weather WHERE searchText = ?;

insertWeather:
INSERT OR REPLACE
INTO Weather(
    id,
    searchText,
    name,
    temp,
    speed,
    humidity,
    pressure
) VALUES(?,?,?,?,?,?, ?);



CREATE TABLE Weather (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    temp REAL NOT NULL,
    speed REAL NOT NULL,
    humidity INTEGER NOT NULL,
    pressure INTEGER NOT NULL
);

getAllWeather:
SELECT *
FROM Weather;

insertWeather:
INSERT OR REPLACE
INTO Weather(
    id,
    name,
    temp,
    speed,
    humidity,
    pressure
) VALUES(1,?,?,?,?,?);


